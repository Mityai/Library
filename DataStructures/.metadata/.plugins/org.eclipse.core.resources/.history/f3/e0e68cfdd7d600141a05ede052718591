#include <bits/stdc++.h>

const int N = int(1e5);

int p[N];

int find(int x) {
	return x == p[x] ? x : p[x] = find(p[x]);
}

void union(int x, int y) {
	x = find(x);
	y = find(y);

	p[y] = x;
}

int main() {
	int n;
	scanf("%d", &n);
}

