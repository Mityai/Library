/*
 * SparseTable.cpp
 *
 * minimum on range
 * [0..n)
 *
 *  Created on: Mar 29, 2015
 *      Author: dima
 */

#include <bits/stdc++.h>

const int N = int(1e5);
const int K = 20;

int a[N];
int ST[K][N];

int main() {
	int n;
	scanf("%d", &n);

	for (int i = 0; i < n; i++) {
		scanf("%d", &a[i]);
	}

	for (int i = 0; i < n; i++) {
		ST[0][i] = a[i];
	}
	for (int k = 0; (1 << (k + 1)) < n; k++) {
		for (int i = 0; i < n; i++) {
			ST[k + 1][i] = min(ST
		}
	}
}
